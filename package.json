{
  "name": "com.benallfree.cordova.arjs.showcase",
  "displayName": "AR.js Showcase",
  "version": "0.0.1",
  "description": "AR.js samples running in Cordova",
  "source": "./src/index.tsx",
  "browser": "./www/js/index.js",
  "browserslist": [
    "last 1 Android version",
    "last 1 iOS version"
  ],
  "scripts": {
    "build": "npm run build:clean && npm run build:parcel && cordova prepare",
    "build:clean": "shx rm -rf www && shx mkdir www",
    "build:parcel": "parcel build ./src/index.html --no-minify --public-url . -d www"
  },
  "keywords": [
    "ecosystem:cordova",
    "aframe",
    "webvr"
  ],
  "author": "Ben Allfree",
  "license": "ICS",
  "dependencies": {
    "cordova-add-scripts": "0.0.1",
    "cordova-ios": "^5.1.1",
    "cordova-plugin-file": "^6.0.2",
    "cordova-plugin-iosrtc": "^6.0.9",
    "cordova-plugin-whitelist": "^1.3.4",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-router-dom": "^5.1.2",
    "reaframe": "0.0.1"
  },
  "devDependencies": {
    "@types/cordova": "0.0.34",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-router-dom": "^5.1.3",
    "chokidar-cli": "^2.1.0",
    "cordova": "^9.0.0",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-json-url-loader": "^0.1.2",
    "prettier": "^1.19.1",
    "shx": "^0.3.2",
    "typescript": "^3.8.3"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-xhr-local-file": {},
      "cordova-plugin-iosrtc": {}
    },
    "platforms": [
      "ios"
    ]
  }
}